                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 200,
                            "message": "Local identity profile updated."
                        },
                        {
                            "code": 400,
                            "message": "The request was improperly formatted or contained invalid fields."
                        },
                        {
                            "code": 403,
                            "message": "PingFederate does not have its IdP role enabled. Operation not available."
                        },
                        {
                            "code": 404,
                            "message": "Resource not found.",
                            "responseModel": "ApiResult"
                        },
                        {
                            "code": 422,
                            "message": "Validation error(s) occurred.",
                            "responseModel": "ApiResult"
                        }
                    ],
                    "summary": "Update the local identity profile by ID.",
                    "type": "LocalIdentityProfile"
                },
                {
                    "method": "DELETE",
                    "nickname": "deleteIdentityProfile",
                    "notes": "Delete a local identity profile with the specified ID. A 404 status code is returned for nonexistent IDs. Note: If the request succeeds, the response body is empty. If the request fails, an ApiResult is returned with details of the error.",
                    "parameters": [
                        {
                            "allowMultiple": false,
                            "description": "ID of the profile to delete",
                            "name": "id",
                            "paramType": "path",
                            "required": true,
                            "type": "string"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 204,
                            "message": "Local identity profile deleted."
                        },
                        {
                            "code": 403,
                            "message": "PingFederate does not have its IdP role enabled. Operation not available."
                        },
                        {
                            "code": 404,
                            "message": "Resource not found.",
                            "responseModel": "ApiResult"
                        },
                        {
                            "code": 422,
                            "message": "Validation error(s) occurred.",
                            "responseModel": "ApiResult"
                        }
                    ],
                    "summary": "Delete the local identity profile by ID.",
                    "type": "ApiResult"
                }
            ],
            "path": "/localIdentity/identityProfiles/{id}"
